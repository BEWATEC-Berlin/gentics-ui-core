{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "noImplicitAny": true,
    "isolatedModules": true,
    "noEmit": true
  },
  "exclude": [
      "dist",
      "node_modules"
  ]
}
